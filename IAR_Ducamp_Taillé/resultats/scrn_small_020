class SmallConfig(object):
  """Petit réseau"""
  init_scale = 0.05
  learning_rate = 0.2
  max_grad_norm = 5
  num_layers = 1
  num_steps = 50
  hidden_size = 90
  context_size = 10
  alpha = 0.95
  max_epoch = 6
  max_max_epoch = 20
  keep_prob = 0.5
  lr_decay = 0.8
  batch_size = 32
  vocab_size = 10000
